<UserControl x:Class="GmudGen.Pages.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="600">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/General.xaml" Cursor="Hand">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="General" Source="/Pages/General.xaml" />
                <mui:Link DisplayName="Artifacts" Source="/Pages/Artifacts.xaml" />
                <mui:Link DisplayName="Notification" Source="/Pages/Notification.xaml" />
                <mui:Link DisplayName="Build GMUD" Source="/Pages/Build.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
